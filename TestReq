import io.swagger.annotations.ApiModel;
import io.swagger.annotations.ApiModelProperty;
import lombok.Data;

import java.util.Date;

/**
 * @author franyang
 * @date 2018/10/9
 */
@Data
@ApiModel("测试接口传参值类")
public class TestReq {
    @ApiModelProperty(name = "name",value = "测试名称",example = "frank",required = true)
    private String name;
    @ApiModelProperty(name = "value",value = "测试值",example = "value")
    private String value;
    @ApiModelProperty(name = "number",value = "测试数字",example = "1")
    private int number;
    @ApiModelProperty(name = "date",value = "测试时间Date",example = "2018-09-09 11:11:11")
    private Date date;
}
